// ウィンドウ座標を取得する
function getWindowPosition(top, width, height) {
	var top = top + (document.body.scrollTop || document.documentElement.scrollTop);
	var left = ((document.body.clientWidth || document.documentElement.clientWidth) - width) / 2;
	if (left < 0)
		left = 0;
	return {top: top, left: left, width: width, height: height};
}

// 開いているウィンドウを閉じる
