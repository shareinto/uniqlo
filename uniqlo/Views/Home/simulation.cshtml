    window.open(url, '_blank', 'location=yes,width=' + width +',height=' + height);
}
// URL表示の切り替え
function toggleUrlFacebook(){
    var url = 'http://www.facebook.com/sharer.php?u=' + encodeURIComponent(share_url);
