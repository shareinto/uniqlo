	clip.glue("d_clip_button", "viewCode");
	clip.setText(share_url);
}

// 汎用クラス切り換え
function switchCss(elm,value){
	(Element.hasClassName(elm,value) == false) 
		? Element.addClassName(elm,value) 
		: Element.removeClassName(elm,value);
}

// si_id取得
function getSid(name) {
	var regexp = new RegExp('; ' + name + '(=([^;]*))?;');
	var match  = ('; ' + document.cookie + ';').match(regexp);
	return match ? match[2] : null;
}

